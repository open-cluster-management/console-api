{
  "name": "console-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./build/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./build/index.js",
    "start:development": "cross-env NODE_ENV=development node ./build/index.js",
    "start:debug": "cross-env NODE_ENV=development node --inspect ./build/index.js",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf build && mkdir build",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/console-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-commit": "make copyright-check && lint-staged",
      "pre-push": "npm run test -- -o"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.17.0",
    "body-parser": "^1.19.0",
    "casual": "^1.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "flat": "^5.0.2",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.1",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.1.0",
    "js-yaml": "^3.14.0",
    "jws": "^4.0.0",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "lru-cache": "^6.0.0",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nocache": "^2.1.0",
    "nock": "^13.0.4",
    "request": "^2.88.2",
    "requestretry": "^4.1.1",
    "security-middleware": "github:open-cluster-management/security-middleware",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
