{
  "name": "console-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --no-notify --no-deps --respawn --transpile-only src",
    "start:production": "cross-env NODE_ENV=production node ./build/src",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./build/src",
    "start:development": "cross-env NODE_ENV=development node ./build/src",
    "start:debug": "cross-env NODE_ENV=development node --inspect ./build/src",
    "build": "npm run clean && tsc && cp config/*.json build/config",
    "build:production": "npm run build",
    "clean": "rimraf build && mkdir build",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext .js --ext .ts ./src",
    "lint:fix": "eslint --ext .js --ext .ts --fix ./src",
    "watch": "npm run start",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/console-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-push": "make copyright-check && npm run test -- -o && npm run lint"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.14.4",
    "body-parser": "^1.19.0",
    "casual": "^1.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "flat": "^5.0.0",
    "graphql": "^15.1.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^3.23.1",
    "js-yaml": "^3.14.0",
    "jws": "^4.0.0",
    "lodash": "^4.17.19",
    "log4js": "^6.3.0",
    "lru-cache": "^5.1.1",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nocache": "^2.1.0",
    "nock": "^12.0.3",
    "request": "^2.88.2",
    "requestretry": "^4.1.1",
    "rimraf": "^3.0.2",
    "security-middleware": "github:open-cluster-management/security-middleware",
    "typescript": "^3.9.6",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.51"
  }
}
