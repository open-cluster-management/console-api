{
  "name": "@open-cluster-management/console-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --no-notify --no-deps --respawn --transpile-only src",
    "start:production": "cross-env NODE_ENV=production node ./build/src",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./build/src",
    "start:development": "cross-env NODE_ENV=development node ./build/src",
    "start:debug": "cross-env NODE_ENV=development node --inspect ./build/src",
    "build": "npm run clean && tsc && cp config/*.json build/config",
    "build:production": "npm run build",
    "clean": "rimraf build",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext .js --ext .ts ./src",
    "lint:fix": "eslint --ext .js --ext .ts --fix ./src",
    "watch": "npm run start",
    "test": "npm run clean && jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/console-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-push": "make copyright-check && npm run test -- -o && npm run lint"
    }
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.16.1",
    "body-parser": "^1.19.0",
    "casual": "^1.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "flat": "^5.0.2",
    "graphql": "^15.3.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.1.0",
    "js-yaml": "^3.14.0",
    "jws": "^4.0.0",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "lru-cache": "^6.0.0",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nocache": "^2.1.0",
    "request": "^2.88.2",
    "requestretry": "^4.1.1",
    "security-middleware": "github:open-cluster-management/security-middleware",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/flat": "^5.0.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.10",
    "@types/js-yaml": "^3.12.5",
    "@types/jws": "^3.2.2",
    "@types/lodash": "^4.14.159",
    "@types/log4js": "^2.3.5",
    "@types/lru-cache": "^5.1.0",
    "@types/moment": "^2.13.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.6.0",
    "@types/request": "^2.48.5",
    "@types/requestretry": "^1.12.7",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "nock": "^13.0.4",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "typescript": "^3.9.7",
    "ts-jest": "26.2.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.57"
  }
}
