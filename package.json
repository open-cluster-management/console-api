{
  "name": "@open-cluster-management/console-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./build/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./build/index.js",
    "start:development": "cross-env NODE_ENV=development node ./build/index.js",
    "start:debug": "cross-env NODE_ENV=development node --inspect ./build/index.js",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf build && mkdir build",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/console-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-commit": "make copyright-check && lint-staged",
      "pre-push": "npm run test -- -o"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.14.4",
    "body-parser": "^1.19.0",
    "casual": "^1.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "flat": "^5.0.0",
    "graphql": "^15.1.0",
    "graphql-tools": "^6.0.10",
    "graphql-type-json": "^0.3.2",
    "helmet": "^3.23.1",
    "js-yaml": "^3.14.0",
    "jws": "^4.0.0",
    "lodash": "^4.17.15",
    "log4js": "^6.3.0",
    "lru-cache": "^5.1.1",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nocache": "^2.1.0",
    "request": "^2.88.2",
    "requestretry": "^4.1.1",
    "security-middleware": "github:open-cluster-management/security-middleware",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/flat": "^5.0.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.4",
    "@types/js-yaml": "^3.12.5",
    "@types/jws": "^3.2.2",
    "@types/lodash": "^4.14.157",
    "@types/log4js": "^2.3.5",
    "@types/lru-cache": "^5.1.0",
    "@types/moment": "^2.13.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.0.20",
    "@types/request": "^2.48.5",
    "@types/requestretry": "^1.12.7",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "lint-staged": "^10.2.10",
    "nock": "^12.0.3",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
