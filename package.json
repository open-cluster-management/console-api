{
  "name": "console-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./build/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./build/index.js",
    "start:development": "cross-env NODE_ENV=development node ./build/index.js",
    "start:debug": "cross-env NODE_ENV=development node --inspect ./build/index.js",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf build && mkdir build",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext ,.js .",
    "lint:fix": "eslint --ext .jsx,.js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/console-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "make copyright-check && lint-staged",
      "pre-push": "npm run test -- -o"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.14.4",
    "body-parser": "^1.19.0",
    "casual": "^1.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "flat": "^5.0.0",
    "graphql": "^15.1.0",
    "graphql-tools": "^6.0.10",
    "graphql-type-json": "^0.3.2",
    "helmet": "^3.23.1",
    "js-yaml": "^3.14.0",
    "jws": "^4.0.0",
    "lodash": "^4.17.15",
    "log4js": "^6.3.0",
    "lru-cache": "^4.1.3",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nocache": "^2.1.0",
    "nock": "^10.0.6",
    "request": "^2.88.2",
    "requestretry": "^4.1.1",
    "security-middleware": "github:open-cluster-management/security-middleware",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.17.0",
    "husky": "^4.2.5",
    "jest": "^23.1.0",
    "jest-tap-reporter": "^1.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
